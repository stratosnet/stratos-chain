{
  "exclude": [
    "**/dist/**",
    "**/node_modules/**",
    "**/test/**"
  ],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2015",
      "es5",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "preserveSymlinks": true,
    "preserveWatchOutput": true,
    "pretty": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es6",
    "strict": true
  },
  "references": [
    {
      "path": "./gov"
    },
  ]
}